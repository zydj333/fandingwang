.join .text{width:398px;height:38px;line-height:38px;background:#FAFAFA;border:1px solid #E6E6E6;border-radius:6px;padding:3px 6px;}
.join ul li{margin-bottom:17px;}
.join select{height:38px;line-height:38px;background:#FAFAFA;border:1px solid #E6E6E6;border-radius:6px;padding:3px 0 3px 6px;width:134px;}
