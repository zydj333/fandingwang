@charset "utf-8";@JOEN;
body,input{margin:0; padding:0; font:14px/170% "汉仪长美黑简","微软雅黑","宋体",Arial; background-color:#fff; color:#666}
img{border:none; vertical-align:top}
dl,dd,dt,ul,li,form,iframe,h1,h2,h3,h4,h5,h6,p{margin:0; padding:0}
ul{list-style:none}
a{text-decoration:none; outline:none; color:#666}
a:hover{color:#333; text-decoration:none}
*:focus{outline:none}
.clear{clear:both; margin:0; padding:0; height:0; line-height:0; font-size:0}
.box{width:1080px; margin:0 auto; position:relative}
.bg{background-color:#000; height:100%; width:100%}
a,.header .nav li,.project .item .left .bg,.share .block .ti,.probanner li,.proDetail .right .item,.header .nav .info dl,.member .left .item{-moz-transition:all .3s ease-in; -webkit-transition:all .3s ease-in; -o-transition:all .3s ease-in; transition:all .3s ease-in}
.backTop,.floatlogin a,.footer .about dt,.footer .about dd,.footer .public p,.project .nav .right .hot,.ti .fav,.LRform .sns a,.log .inp{background-image:url(/images/ico.png); background-repeat:no-repeat}
.project .item .left .bg,.share .block .ti,.share .block,.service .block,.header .nav .info dl{opacity:0; filter:progid:DXImageTransform.Microsoft.Alpha(opacity=0)}
.floatlogin .close:hover,.share .block .pic:hover .ti{opacity:1; filter:progid:DXImageTransform.Microsoft.Alpha(opacity=100)}
.header,.footer,.banner,.probanner,.prolaunch,.proDbanner,.sharebanner,.memberbanner,.share,.pages{min-width:1080px}
.service,.project,.proDetail,.share{margin-bottom:70px}

/*颜色*/
.cor_blue{color:#35bacb}
.cor_fc0,.header .login a:hover{color:#fc0}
.cor_f00,.Wrong{color:#f00}

/*表单*/
.inp{border:#e6e6e6 solid 1px; border-radius:5px; height:36px; width:262px; padding:0 12px; background-color:#fff}
.inp01{width:152px}
.inp02{width:98px}
.sub{border-color:#fc0; background-color:#fc0; text-align:center; color:#fff; font-size:16px; cursor:pointer; width:286px; padding:0}
.but .sub{width:96px; float:right; margin-top:12px}
.con textarea{width:96%; height:70px; padding:8px 2% 0 2%; border:none; background-color:#f7f7f7; overflow:auto}

/*头部 尾部*/
.header{height:64px; color:#fff; width:100%}
.header a{color:#fff}
.header .box{height:100%; margin-top:-64px}
.header .box div{position:absolute; z-index:20}
.header .logo{left:2px; top:8px}
.header .nav{right:2px; top:0}
.header .nav ul:hover .now{border-top:none; margin-top:2px}
.header .nav li{float:left; margin-top:2px}
.header .nav li:hover,.header .nav .now,.header .nav ul:hover .now:hover{border-top:#fc0 solid 2px; margin-top:0}
.header .nav li:hover .chat{display:block}
.header .nav a{display:block; padding:3px 22px; margin-top:15px}
.header .nav .chat{display:none; padding-top:18px; margin-left:-55px}
.header .nav .launch a{border:#434141 solid 1px; border-radius:16px}
.header .nav .login,.header .nav .info{margin:20px auto auto 28px}
.header .nav .login:hover,.header .nav .info:hover{border-top:none; margin-top:20px}
.header .nav .login a{display:inline; padding:0}
.header .nav .login span{padding:0 8px}
.header .nav .info{border-left:#3b3b3b solid 1px; position:relative; z-index:10; height:27px; padding-right:16px}
.header .nav .info img{width:32px; height:32px; border-radius:60px; border:#fff solid 2px; margin:-5px 12px auto 26px}
.header .nav .info dl{background-color:#000; width:128px; height:0; overflow:hidden; position:absolute; top:31px; right:0}
.header .nav .info dl a{padding:0 0 0 36px}
.header .nav .info:hover dl{height:170px; opacity:0.8; filter:progid:DXImageTransform.Microsoft.Alpha(opacity=80)}

.backTop{display:none; cursor:pointer; width:60px; height:60px; background-color:#3f4041; position:fixed; z-index:99; bottom:158px; right:50px; background-position:-51px -258px}
.backTop:hover{background-color:#000}

.footer{background-color:#2d2f31; color:#969798; padding-bottom:45px;margin-top: 70px;}
.footer a{color:#969798}
.footer a:hover{color:#fff}
.footer .ban{height:336px; background:url(/images/footban.jpg) no-repeat center top}
.footer .box{padding-top:55px}
.footer .box div{float:left; height:218px; border-left:#424446 solid 1px; border-right:#09090a solid 1px; padding:0 45px; width:178px; text-align:center}
.footer .box .logo{border-left:none}
.footer .logo img{margin-top:36px}
.footer .box .about{text-align:left}
.footer .about dt{font-size:18px; line-height:44px; background-position:0 -67px; margin-bottom:28px}
.footer .about dd{margin-top:6px; background-position:0 -53px; padding-left:15px}
.footer .box .public{border-right:none}
.footer .public p{background-position:left 2px; padding-left:34px; display:inline-block; margin-top:7px}
.footer .public .wechat{background-position:0 -30px}
.footer .public .code{padding-left:0; background:none; margin-top:30px}
.footer .copyright{text-align:center; margin-top:55px}

/*浮动登录*/
.floatlogin{height:68px; width:100%; position:fixed; z-index:99; left:0; bottom:0; display:none}
.floatlogin .bg{opacity:0.7; filter:progid:DXImageTransform.Microsoft.Alpha(opacity=70)}
.floatlogin .box{height:100%; margin-top:-68px}
.floatlogin a{display:inline-block; width:138px; height:34px; margin:17px auto auto 14px}
.floatlogin .text{width:223px; height:24px; background-position:0 -133px; margin:23px auto auto 32px}
.floatlogin .douban{background-position:0 -179px; margin-left:94px}
.floatlogin .weibo{background-position:-152px -179px}
.floatlogin .qq{background-position:0 -224px}
.floatlogin .mail{background-position:-152px -224px}
.floatlogin .close{margin:0; width:36px; height:31px; background-position:0 -268px; position:absolute; bottom:19px; right:53px; opacity:0.6; filter:progid:DXImageTransform.Microsoft.Alpha(opacity=60)}

/*海报*/
.banner{position:relative; height:554px}
.banner .slide{position:relative; overflow:hidden; height:100%}
.banner .slide li{position:absolute; display:none; width:100%; height:100%; left:0; top:0; background-position:center top; background-repeat:no-repeat}
.banner .slide a{display:block; width:100%; height:100%}  
.banner .slide_nav{position:absolute; top:50%; margin-top:-35px; left:0; opacity:0; filter:progid:DXImageTransform.Microsoft.Alpha(opacity=0); z-index:3; cursor:default; text-indent:-9999px; overflow:hidden; height:69px; width:41px; background:transparent url(/images/but.png) no-repeat left top}
.banner .slide_nav.next{left:auto; background-position:right top; right:0}
.banner:hover .slide_nav{left:15%; opacity:1; filter:progid:DXImageTransform.Microsoft.Alpha(opacity=100)}
.banner:hover .slide_nav.next{left:auto; right:15%}
  
/*标题部分*/
.title{text-align:center; margin:50px auto 24px}
.title h1{font-size:32px; color:#2e301c; line-height:56px}
.title h1 span{color:#ffcc00; text-transform:uppercase; display:inline-block; margin-left:5px}
.title .more a{text-decoration:underline}
.title .more a:hover{text-decoration:none}

/*项目部分*/
.project .nav{border-top:#d9d9d9 solid 1px; height:60px}
.project .nav li{float:left}
.project .nav .left li{padding:21px 39px 0; margin-top:-3px}
.project .nav .left .now{border-top:#ffcc00 solid 5px; padding-top:16px}
.project .nav .right{float:right}
.project .nav .right li,.share .right .nav li{margin:15px auto auto 12px; border:#d9d9d9 solid 1px; border-radius:16px; padding:0 17px; font-size:12px}
.project .nav .right .hot{background-position:-251px -272px; padding:0 13px 0 23px}
.project .nav .right .now,.share .right .nav .now{background-color:#ffcc00; border-color:#ffcc00; color:#fff}
.project .nav .right .more{border-radius:0; padding:3px 20px}
.project .item{clear:both; height:300px; margin-top:40px}
.project .item .left{width:486px; height:100%; float:left; background-position:center center; background-repeat:no-repeat; position:relative}
.project .item .left .process{opacity:0; display:none\9;*display:none; width:100%; height:100%; background:url(/images/join.png) no-repeat center center; position:absolute; z-index:2; left:0; top:0}
.project .item .left .waitingstart{opacity:0; display:none\9;*display:none; width:100%; height:100%; background:url(/images/join1.png) no-repeat center center; position:absolute; z-index:2; left:0; top:0}
.project .item .left .overed{opacity:0; display:none\9;*display:none; width:100%; height:100%; background:url(/images/join2.png) no-repeat center center; position:absolute; z-index:2; left:0; top:0}
/*.project .item .left:hover a{opacity:1; display:block}*/
.project .item .left .bg{position:absolute; z-index:1; left:0; top:0}
.project .item .left:hover .bg{opacity:0.7; filter:progid:DXImageTransform.Microsoft.Alpha(opacity=70)}
.project .item .right{float:right; width:540px; height:100%; margin-right:9px; position:relative}
.project .item .right .ti{line-height:38px; font-size:18px; color:#0f0c20; margin-bottom:8px}
.project .item .right .ti h2{font-size:28px}
.project .item .right .ti span{font-size:12px; margin-left:12px; color:#999}
.project .item .right .ti .fav,.share .detail .ti .fav{width:20px; height:19px; display:block; position:absolute; right:12px; top:14px; background-position:-151px -276px}
.project .item .right .ti .fav:hover,.share .detail .ti .fav:hover{background-position:-171px -276px}
.project .item .right .con{line-height:24px; margin-bottom:8px}
.project .item .right .rate{border-bottom:#d9d9d9 solid 1px; color:#999; font-size:12px; width:100%; line-height:36px; height:36px}
.project .item .right .rate li{float:left; width:24%}
.project .item .right .rate,.project .item .right .bar{position:absolute; left:0; bottom:17px}
.project .item .right .bar,.proDetail .right .bar,.member .item .rate .bar{height:2px; background-color:#fc0; font-size:0; line-height:0}

/*社区*/
#share{background-color:#f8f8f8; padding:1px 0 80px; margin-top:82px}
.share .ico,.proDetail .comment .ico{background:url(/images/share_ico.png) no-repeat}
.share .head img{width:55px; height:55px; border-radius:100px}
.share .block{width:348px; height:358px; background-color:#fff; border:#e0e0e0 solid 1px; float:left; margin:16px 15px auto auto}
.share .block .pic{width:100%; height:250px; overflow:hidden; background-position:center center; background-repeat:no-repeat; position:relative}
.share .block .pic a{display:block; width:100%; height:100%}
.share .block .pic .ti{height:40px; width:100%; line-height:40px; overflow:hidden; color:#fff; font-size:16px; text-align:center; background-color:#000; background:rgba(0,0,0,0.7); position:absolute; left:0; bottom:-40px}
.share .block .pic:hover .ti{bottom:0}
.share .block .info{position:relative}
.share .block .info div{position:absolute}
.share .block .head{left:21px; top:22px}
.share .block .name{left:85px; top:30px}
.share .block .name a{display:block}
.share .block .name .time,.share .item .name{font-size:12px; color:#999}
.share .count{right:18px; top:53px; font-size:12px; color:#999}
.share .count span,.comment .ti span{padding-left:18px; margin-left:10px; display:inline-block}
.share span.browse{background-position:0 5px}
.share span.comment{background-position:0 -22px; margin-top:0}
.share span.favorite{background-position:2px -52px}
.sharebanner{height:300px; margin-top:30px}
.share .left,.share .right{width:240px; float:left; border-top:#ebebeb solid 1px; padding-top:20px; margin-top:67px}
.share .left li{text-align:center}
.share .left li a{padding:12px; display:inline-block}
.share .left li a:hover{font-weight:bold; color:#333}
.share .left .now a{border-bottom:#fc0 solid 2px}
.share .right{width:760px; float:right; overflow:hidden}
.share .right .nav{height:38px}
.share .right .nav li{float:left; margin:8px 12px auto 0}
.share .item{height:310px; width:690px; margin-top:42px; padding-left:70px; border-bottom:#ebebeb solid 1px; position:relative}
.share .item .Sico{position:absolute; top:2px; right:2px; width:60px; height:25px; text-align:center; line-height:25px; border-radius:16px; font-size:12px}
.share .item .top{background-color:#fc0; color:#fff}
.share .item .head,.share .detail .head{position:absolute; left:0; top:0}
.share .item .ti,.share .detail .ti h2{font-size:20px; line-height:32px; font-weight:normal; color:#000}
.share .item .ti a{color:#000}
.share .item .ti a:hover{color:#666}
.share .item .name{margin-bottom:30px}
.share .item .name span{margin-left:18px}
.share .item .con{width:400px; float:right; font-size:12px; line-height:26px}
.share .item .pic,.share .item .pic img{width:248px; height:178px}
.share .item .count{position:absolute; top:235px}
.share .detail{position:relative; margin-top:32px}
.share .detail .ti{width:690px; padding-left:70px; font-size:12px; color:#999}
.share .detail .ti span{margin-left:18px}
.share .detail .ti .count{display:inline-block}
.share .detail .con{margin-top:40px; font-size:12px; line-height:28px}
.comment{margin-top:45px}
.comment .ti{font-size:20px}
.comment .ti .comment{float:right; margin:0 10px 0 0; font-size:12px}
.comment .con{margin-top:10px; height:132px}
.comment .con textarea{width:738px; height:110px; padding:10px; background-color:#f7f7f7; border:#f7f7f7 solid 1px; overflow:auto}
.commentlist .item{font-size:12px; height:auto; border-bottom-style:dotted; padding-bottom:30px}
.commentlist .reply{float:right}
.share .commentlist .name{margin-bottom:0}
.share .commentlist .con{width:100%; float:none; color:#000}
.commentlist .Rcon{background-color:#f7f7f7; padding:10px 15px 10px 20px; margin-top:10px}
.commentlist .more{text-align:center; margin-top:10px}

/*服务*/
.service .block{width:220px; padding-left:104px; background:url(/images/service_ico.png) no-repeat; float:left; margin:30px auto auto 54px}
.service .block dt{font-weight:bold; color:#2e301c; font-size:24px; line-height:46px}
.service .ico01{margin-left:0; background-position:0 12px}
.service .ico02{background-position:0 -301px}
.service .ico03{background-position:0 -613px}

/*分页*/
.pages{clear:both; text-align:center; margin-top:70px;}
.right .pages,.left .pages{min-width:inherit}
.pages a{display:inline-block; padding:5px 3px; min-width:28px; border:#f2f2f2 solid 1px; border-radius:50px; margin:0 2px}
.pages a:hover{background-color:#fc0; color:#fff; border-color:#fc0}
.pages .now{background-color:#fc0}

/*项目集市*/
.probanner{height:430px}
.probanner li{float:left; width:180px; height:180px; margin:145px 45px auto}
.probanner .li01{background-image:url(/images/projects_ban01.png)}
.probanner .li02{background-image:url(/images/projects_ban02.png)}
.probanner .li03{background-image:url(/images/projects_ban03.png)}
.probanner .li04{background-image:url(/images/projects_ban04.png)}
.probanner .li01:hover{background-image:url(/images/projects_aban01.png)}
.probanner .li02:hover{background-image:url(/images/projects_aban02.png)}
.probanner .li03:hover{background-image:url(/images/projects_aban03.png)}
.probanner .li04:hover{background-image:url(/images/projects_aban04.png)}
.prolaunch{background-color:#171717; height:60px; line-height:60px; color:#fff; font-size:16px}
.prolaunch .but{position:absolute; right:3px; bottom:12px; width:180px; height:36px; display:block; line-height:36px; text-align:center; color:#fff; border-radius:18px; background:url(/images/launch_but.png) repeat-x}

/*项目详情*/
.proDbanner{height:390px}
.proDetail{margin-top:70px}
.proDetail .nav{border-top:#ebebeb solid 1px; height:56px; line-height:56px}
.proDetail .nav li{float:left; padding:0 34px}
.proDetail .nav .now{border-top:#fc0 solid 2px; margin-top:-1px}
.proDetail .con{font-size:12px; width:100%; overflow:hidden; margin-top:36px}
.proDetail .head img{width:50px; height:50px; border-radius:100px}
.proDetail .left{width:680px; float:left; line-height:28px}
.proDetail .left .time{width:200px; height:31px; line-height:31px; text-align:center; background-color:#f7f7f7; border-radius:18px; margin-bottom:20px; clear:both}
.proDetail .left .content{margin-bottom:70px; clear:both}
.proDetail .right{width:320px; float:right; color:#999}
.proDetail .right .nav{text-align:right}
.proDetail .right .ti{color:#333; font-size:24px}
.proDetail .right .ti span{font-size:36px; font-family:Arial; display:inline-block; line-height:40px}
.proDetail .right .ti .info{font-size:18px}
.proDetail .right .head{padding-bottom:40px}
.proDetail .right .rate,.member .item .rate{border-top:#d9d9d9 solid 1px; width:100%; line-height:36px; height:36px; position:relative; margin-top:36px}
.proDetail .right .rate li,.member .item .rate li{float:left; width:30%; text-align:center}
.proDetail .right .rate li:first-child,.member .item .rate li:first-child{text-align:left; width:30%}
.proDetail .right .rate li:last-child,.member .item .rate li:last-child{text-align:right; width:40%}
.proDetail .right .rate .bar,.member .item .rate .bar{position:absolute; left:0; top:-2px}
.proDetail .right .description{margin-top:26px}
.proDetail .right .but{display:block; width:100%; height:38px; line-height:38px; background-color:#fc0; text-align:center; color:#fff; font-size:20px; border-radius:22px; margin-top:30px}
.proDetail .right .item{padding:10px 6px 20px; border-radius:8px; margin-top:20px}
.proDetail .right .item:hover{background-color:#f0f0f0}
.proDetail .right .item .block{margin-top:13px}
.proDetail .comment .con{margin-top:10px}
.proDetail .comment .ico{background-position:0 -20px; margin-top:0}
.proDetail .commentlist .item{position:relative; width:610px; padding-left:70px; border-bottom:#ebebeb solid 1px; margin-top:42px}
.proDetail .commentlist .item .con{margin-top:0}
.proDetail .commentlist .head{position:absolute; left:0; top:0}
.proDetail .commentlist .time{background-color:transparent; margin-left:15px}
.proDetail .support .item{margin-top:20px; padding-bottom:0; border-bottom:none}
.proDetail .support .pages{border-top:#ebebeb solid 1px; margin-top:60px; padding-top:50px}

/*登录&注册*/
.LRform{width:548px; margin:112px auto; padding:40px; background-color:#f7f7f7; border-radius:10px}
.LRform h1{color:#333; font-size:28px; text-align:center; line-height:60px}
.LRform .sns{text-align:center; border-bottom:#ccc dashed 1px; height:100px; margin-top:10px}
.LRform .sns a{display:inline-block; width:61px; height:67px; margin:0 25px}
.LRform .sns a:hover{opacity:0.8; filter:progid:DXImageTransform.Microsoft.Alpha(opacity=80)}
.LRform .sns .douban{background-position:0 -325px}
.LRform .sns .weibo{background-position:-112px -325px}
.LRform .sns .qq{background-position:-223px -325px}
.LRform .sns .note{margin:15px auto auto; width:165px; background-color:#f7f7f7}
.LRform .form{width:288px; margin:60px auto auto; font-size:12px}
.LRform .form li{margin-top:12px}
.LRform .form .forget{float:right}
.LRform .bot{text-align:right; margin-top:40px; font-size:12px}
.log .inpname{padding-left:40px; width:234px; background-position:-286px -361px}
.log .inppw{padding-left:40px; width:234px; background-position:-286px -320px}
.log .sub{background-image:none}
.reg h1{background:url(/images/reg_ico.png) no-repeat center bottom; padding-bottom:20px}
.reg .form{margin-top:36px}
.reg .form li{margin-top:10px}
.reg .form .inp02{float:right; font-size:12px; padding:0; text-align:center; cursor:pointer}
.reg .agree{text-align:center; margin-top:16px}
.reg .sns{border-bottom:none; border-top:#ccc dashed 1px; margin-top:30px; height:auto}
.reg .sns .note{margin:26px auto 36px}

/*会员中心*/
.memberbanner{height:390px; color:#fff; position:relative}
.memberbanner .name{text-align:center; position:absolute; left:0; bottom:45px; width:100%}
.memberbanner .name span{border-bottom:#77716c solid 1px; display:inline-block; font-size:16px; height:26px}
.member,.main{background-color:#fafafa; padding:20px 0 110px}
.member .block{background-color:#fff; border:#ebebeb solid 1px; padding:38px; margin-top:40px}
.member .left{width:720px; float:left}
.member .left .ico{background:url(/images/share_ico.png) no-repeat; padding-left:18px; margin-right:33px}
.member .left .comment{background-position:0 -23px}
.member .left .bbs{background-position:0 -83px}
.member .left .pro{background-position:0 -117px}
.member .left .con,.member .right .con{margin-top:10px; height:78px}
.member .left .nav{height:36px; border-bottom:#ebebeb solid 1px}
.member .left .nav li{float:left; padding:0 35px; height:36px}
.member .left .nav .now{border-bottom:#fc0 solid 2px}
.member .left .count{height:56px; line-height:56px; font-size:12px; clear:both}
.member .left .count li{float:left; margin:0 30px 0 10px}
.member .left .count .now{color:#fc0}
.member .left .proti{background-color:#fafafa; height:36px; line-height:36px; border-top:#ebebeb solid 1px; padding-left:27px; font-size:12px}
.member .left .fr{float:right; display:inline-block; width:80px; text-align:center}
.member .left .fr01{margin-right:100px}
.member .left .fl{float:left}
.member .left .item{height:106px; padding:39px 0; border-bottom:#ebebeb solid 1px; position:relative; font-size:12px}
.member .left .item:hover{background-color:#fafafa}
.member .item .pic img{width:156px; height:106px}
.member .item .state{width:60px; text-align:center; border:#ebebeb solid 1px; background-color:#fff; border-radius:10px; position:absolute; left:408px; top:39px; color:#999}
.member .item .fav{position:absolute; right:32px; top:80px; width:20px; height:20px; background:url(/images/ico.png) no-repeat -171px -276px}
.member .item dl{width:290px; position:absolute; left:178px; top:39px}
.member .item dt,.member .item .money b,.member .item .money span{font-size:14px; color:#000; font-weight:normal}
.member .item .money{color:#999; margin-top:24px}
